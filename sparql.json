{
    "queries": [{
        "name": "Count Triple",
        "sparql": "SELECT (count(DISTINCT ?s) as ?count)\nWHERE {\n  ?s a ?o .\n}"
    }, {
        "name": "Search Subject URI",
        "sparql": "SELECT DISTINCT ?s\nWHERE {\n  ?s ?p ?o .\n  FILTER(regex(str(?s), 'corporatebond', 'i')) .\n}"
    }]
}